# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = [NAME GOES HERE]
	email = [EMAIL GOES HERE]
[push]
	default = current
	autoSetupRemote = true
[pull]
	rebase = false
[fetch]
	prune = true
[core]
	editor = code
[init]
	defaultBranch = main
